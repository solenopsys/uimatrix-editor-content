{
  "name": "content",
  "projectType": "library",
  "sourceRoot": "libs/uimatrix/editors/content/src",
  "prefix": "ui",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/uimatrix/editors/content"
      ],
      "options": {
        "jestConfig": "libs/uimatrix/editors/content/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/uimatrix/editors/content/src/**/*.ts",
          "libs/uimatrix/editors/content/src/**/*.html"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  },
  "tags": []
}
